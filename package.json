{
  "name": "media-vault",
  "version": "1.0.0",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch . --ignore 'node_modules build dist .git logs tmp/*' --exec 'node app.js'",
    "start": "node app.js",
    "up": "NODE_ENV=development npx sequelize-cli db:migrate",
    "down": "NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "seed": "NODE_ENV=development npx sequelize-cli db:seed:all",
    "create-model": "cross-env DATABASE_NAME=$npm_config_database npx sequelize-cli model:generate --name $npm_config_name --attributes $npm_config_columns",
    "alter-column": "cross-env DATABASE_NAME=$npm_config_database npx sequelize-cli migration:generate --name alter-column-name --table $npm_config_table --column $npm_config_column --new-type $npm_config_new_type",
    "delete-column": "cross-env DATABASE_NAME=$npm_config_database npx sequelize-cli migration:generate --name delete-column-name --table $npm_config_table --column $npm_config_column",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/type-provider-typebox": "^4.1.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.5.1",
    "js-doc": "^0.5.0",
    "postgres": "^3.4.4",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "husky": "^8.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "tap": "^21.0.1"
  }
}
