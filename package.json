{
  "name": "media-vault",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --watch . --ignore 'node_modules build dist .git logs tmp/*' --exec 'node server.js'",
    "start": "node server.js",
    "up": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "down": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
    "seed-all": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
    "seed-one": "cross-env NODE_ENV=development npx sequelize-cli db:seed --seed",
    "create-model": "npx sequelize-cli model:generate --name $npm_config_name --attributes $npm_config_columns",
    "alter-column": "npx sequelize-cli migration:generate --name alter-column-name -- --table=$npm_config_table --column=$npm_config_column --new-type=$npm_config_new_type",
    "rename-column": "npx sequelize-cli migration:generate --name rename-$npm_config_column-to-$npm_config_new_column -- --table=$npm_config_table",
    "add-column": "npx sequelize-cli migration:generate --name add-$npm_config_new_column-to-$npm_config_table -- --table=$npm_config_table",
    "delete-column": "npx sequelize-cli migration:generate --name delete-column-name --table $npm_config_table -- --column $npm_config_column"
  },
  "keywords": [
    "node js",
    "jwt",
    "authentication",
    "express",
    "postgresql",
    "fileupload"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/auth": "^4.3.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/static": "^6.11.0",
    "@fastify/swagger": "^8.9.0",
    "@fastify/swagger-ui": "^1.9.3",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fastify": "^5.0.0",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.5.1",
    "fastify-socket.io": "^4.0.0",
    "js-doc": "^0.5.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.4",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "tap": "^21.0.1"
  }
}
